include ../data/data
include ../mixin/auth
#over
header
    - const data = [{name:"FileBoom.me",img:"fileboom-premium-key-200x75.png"},{name:"Keep2share.cc",img:"keep2share-premium-key-200x75.png"},{name:"Filenext.Com",img:"filenext-premium-200x75.png"},{name:"File.al Premium",img:"file-all-premium-key-200x75.png"},{name:"Katfile.Com",img:"Katfile-premium-200x75.png"},{name:"Filejoker.net",img:"filejoker-premium-key-200x75.png"},{name:"CloudGhost.Net",img:"cloudghost-premium-200x75.png"},{name:"AllDebrid.com",img:"alldebrid-premium-key-200x75.png"},{name:"Rapidgator.net",img:"rapidgator-premium-key-200x75.png"},{name:"Filesmonster.com",img:"filesmonster-premium-key-200x75.png"},{name:"Filespace.com",img:"filespace-premium-key-200x75.png"},{name:"Xubster.com",img:"xubster-com-premium-200x75.png"},{name:"DepositFiles.com",img:"depositfiles-premium-key-200x75.png"},{name:"TurboBit.net",img:"turbobit-premium-key-200x75.png"},{name:"Novafile.com",img:"novafile-premium-key-200x75.png"},{name:"UploadBoy.com",img:"uploadboy-premium-key-200x75.png"},{name:"DebridItalia.Com",img:"debriditalia-premium-200x75.png"},{name:"Daofile.com",img:"daofile-premium-key-200x75.png"},{name:"EasyBytez.com",img:"easybytez-premium-key-200x75.png"},{name:"Filefactory.com",img:"filefactory-premium-key-200x75.png"},{name:"Filevega.Com",img:"filevega-premium-200x75.png"},{name:"Hotlink.cc",img:"hotlink.cc-paypal-reseller-200x75.png"},{name:"Mountfile.net",img:"mountfile-premium-key-200x75.png"},{name:"Premiumax.Net",img:"Premiumax-premium-200x75.png"},{name:"Rapidrar.Com",img:"Rapidrar-premium-200x75.png"},{name:"Real-Debrid.com",img:"real-debrid-premium-key-200x75.png"},{name:"Subyshare.com",img:"subyshare-premium-key-200x75.png"},{name:"Takefile.link",img:"takefile-paypal-reseller-200x75.png"},{name:"Upload.cd",img:"upload-cd-premium-key-200x75.png"},{name:"Hitfile.net",img:"hitfile-premium-key-200x75.png"},{name:"Ex-load.com",img:"ex-load-premium-key-200x75.png"}]
    .container-fluid
        .row.align-items-center.pt-2
            .col-md-3.order-md-1.order-1
                a.header--logo.d-block.text-md-left.text-center(href="/")
                    img.w-100(src="/dist/images/logo.png", alt="")
            .col-md-6.order-md-2.order-3.px-md-auto.px-0
                .d-block.header--search
                    form(action="", method="POST")
                        .form-group.mb-0
                            input.form-control.rounded-0.border-0.py-sm-auto.py-3(type="text", placeholder="Find Product")
                            button(type="submit")
                                i.fa.fa-search
                aside#aside.pt-md-2.d-block
                    ul.nolist.d-md-inline-flex.w-100.header--menu.text-md-center.text-left
                        li
                            a.active(href="/") Home
                        li
                            a(href="/") Services
                        li
                            a(href="/") News
                        li
                            a(href="/") Contact        
            .col-md-3.align-items-center.d-flex.text-right.nav--mobile.px-md-0.order-md-3.order-2
                a.d-sm-none.d-md-none.flex-sm-1.ml.md-3.text-center(href="/")
                    i.fa.fa-home.fa-2x
                #opCart.flex-sm-1.d-block.header--cart.py-md-1.ml-md-3.pl-md-3.pr-md-1(toggle-menu="opCart", over)
                    .d-inline-flex.align-items-center
                        .d-md-block.d-none.cart--info
                            span.d-md-inline-block.mr-md-2 0 item(s)
                            span.d-md-inline-block.mr-md-2 -
                            span.d-md-inline-block $0.00
                        span.d-block.cart--icon
                            i.fa.fa-shopping-cart.fa-lg
                    .opCart.px-3
                        .row
                            .col-md-12.text-dark.px-0
                                ul#listcart.nolist.order--listcart.px-3(jqueryScroll)
                                    each val in data
                                        li.row.p-1
                                            .col-md-3.d-md-block.d-none.px-0
                                                img.d-block.w-100(src="../../dist/images/logo/" + val.img, alt=val.name)
                                            .col-10.col-md-7-0=val.name
                                            .col-1.px-0 x1
                                            .col-1.px-0
                                                a(ype="button")
                                                    i.fa.fa-close.fa-sm
                            .col-md-12.bg-secondary.px-0
                                a.btn.btn-block.btn-info.btn-lg.text-uppercase.text-light(href="/payment.html")
                                    |checkout
                a.flex-1.d-md-inline-block.ml.md-3.text-center(href="/auth.html")
                    i.fa.fa-user-circle.fa-2x
                a.d-sm-none.d-md-none.flex-sm-1.ml.md-3.text-center(toggle-menu="aside", over)
                    i.fa.fa-bars.fa-2x
